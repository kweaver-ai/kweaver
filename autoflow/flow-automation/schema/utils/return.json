{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "停止运行并输出结果",
    "description": "",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "title": {
            "type": "string"
        },
        "operator": {
            "type": "string",
            "pattern": "^@.*",
            "maxLength": 255
        },
        "parameters": {
            "type": "object",
            "properties": {
                "result": {
                    "type": "string",
                    "enum": ["success", "failed"]
                }
            },
            "required": ["result"],
            "additionalProperties": true
        }
    },
    "required": ["id", "operator", "parameters"],
    "additionalProperties": false
}
